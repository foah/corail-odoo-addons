<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <!-- Chart template -->
        <record id="tn_chart_template" model="account.chart.template">
            <field name="name">Tunisian Chart of Accounts</field>
            <field name="bank_account_code_prefix">114</field>
            <field name="cash_account_code_prefix">111</field>
            <field name="transfer_account_code_prefix">117</field>
            <field name="currency_id" ref="base.TND"/>
            <field name="spoken_languages" eval="'fr_FR;en_EN;ar'"/>
        </record>
</odoo>
